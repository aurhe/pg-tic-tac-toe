{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 4,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "-W106": false, // is not in camel case - to be able to override limejs code
  "globals": {
    "goog": false,
    "lime": false,

    "PG": false,
    "tictactoe": false,
    "createStateForNewMatch": false,
    "evaluateMove": false,

    "describe": false,
    "it": false,
    "waitsFor": false,
    "runs": false,
    "expect": false
  }
}
